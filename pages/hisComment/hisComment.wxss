/* pages/beforeComplete/beforeComplete.wxss */

page {
    width: 100%;
    height: 100%;
}
.wrap {
    width: 100%;
    padding: 50rpx 0 10rpx;
    min-height: 100%;
    background-color: #eaf9fa;
}
.list {
    width: 85%;
    background-color: #ffffff;
    margin: auto;
    box-shadow: 0 5px 10px rgba(222, 222, 222, 0.4);
    margin-bottom: 40rpx;
    position: relative;
}
.list>view {
    text-align: center;
    padding: 35rpx 0;
}
.list>view:first-child {
    width: 66%;
    font-size: 24rpx;
    text-align: justify;
}
.head {
    width: 115rpx;
    height: 115rpx;
    border: 1px solid #55683f;
    border-radius: 50%;
    overflow: hidden;
    position: absolute;
    left: 25rpx;
    top: 25rpx;
}
.msg {
    font-size: 24rpx;
    color: #383837;
    line-height: 45rpx;
    width: 100%;
    margin-left: 165rpx;
    position: relative;
}
.msg>view:first-child {
    font-size: 32rpx;
    font-weight: bold;
    margin-bottom: 10rpx;
}
.list>view:first-child>view:first-child image {
    width: 100%;
    height: 100%;
}
.starAndText {
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    font-size: 0rpx;
    height: 45rpx;
}
.starBox view {
    width: 33rpx;
    display: inline-block;
    margin-left: 5rpx;
    margin-bottom: 0!important;
}
.starBox view image {
    width: 100%;
}
.starText {
    color: #80e6f4;
    font-size: 24rpx;
    text-align: right;
}
.starText text {
    text-decoration: underline;
}
.starBox {
    margin-bottom: 0!important;
    margin-top: 5rpx;
}
.noTips {
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    display: -webkit-flex;
    -webkit-align-items: center;
    -webkit-justify-content: center;
    -webkit-flex-direction: column;
}
.noTips text {
    display: block;
    color: #363838;
    font-size: 26rpx;
    font-weight: normal;
    text-decoration: underline;
}
.hasTips {
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    display: -webkit-flex;
    -webkit-align-items: center;
    -webkit-justify-content: center;
    -webkit-flex-direction: column;
}
.hasTips text {
    display: block;
    width: 104rpx;
    font-size: 26rpx;
    text-decoration: underline;
    padding-left: 40rpx;
    background: no-repeat left center;
    background-size: 28rpx auto;
    background-image:url("data:image/png;base64,");
    
}
#popComment {
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    height: 100%;
    width: 100%;
    z-index: 10;
    background-color: rgba(0, 0, 0, 0.7);
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    display: -webkit-flex;
    -webkit-align-items: center;
    -webkit-justify-content: center;
    -webkit-flex-direction: column;
}
#popComment form {
    width: 85.6%;
}
.popComment {
    width: 100%;
    position: relative;
    padding: 90rpx 85rpx 80rpx;
    border-radius: 6rpx;
    background: no-repeat left bottom;
    background-size: 100% auto;
    background-color: white;
    background-image: url('data:image/png;base64,');
}
.msgHead {
    width: 160rpx;
    height: 160rpx;
    overflow: hidden;
    border-radius: 50%;
    border: 4rpx solid #1d3600;
}
.msgHead image {
    width: 100%;
}
.commentMsg {
    position: relative;
}
.msgRight {
    position: absolute;
    right: 0;
    top: 0;
    width: 55%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    display: -webkit-flex;
    -webkit-align-items: center;
    -webkit-justify-content: center;
    -webkit-flex-direction: column;
}
.msgName {
    font-size: 36rpx;
    color: #333333;
    line-height: 50rpx;
    width: 100%;
    font-weight: bold;
}
.msgId {
    color: #999999;
    font-size: 28rpx;
    line-height: 50rpx;
    width: 100%;
}
.starComment {
    margin-top: 40rpx;
}
.starComment text {
    float: left;
    color: #272727;
    font-size: 30rpx;
    line-height: 60rpx;
    margin-left: 20rpx;
    font-weight: bold;
}
.starComment .starBoxes {
    float: left;
}
.starBoxes view {
    width: 49.5rpx;
    margin: 6.25rpx 2rpx;
    display: inline-block;
    height: 46.5rpx;
    position: relative;
}
.starBoxes view image {
    width: 100%;
    font-size: 0rpx;
}
.starBoxes view image.starUp {
    position: absolute;
    top: 0;
    left: 0;
}
.commentStatus text {
    display: block;
    width: 100%;
    text-align: right;
    color: #b5b5b5;
    font-size: 26rpx;
    font-weight: normal;
    margin-left: 0;
}
.comArea {
    border: 1px solid #aaaaaa;
    border-radius: 6rpx;
    padding: 20rpx;
    margin-top: 20rpx;
}
.comArea textarea {
    width: 100%;
    height: 160rpx;
    font-size: 24rpx;
    text-align: justify;
    color: #888888;
    line-height: 36rpx;
}
.comfirmBtn {
    width: 90%;
    margin: 50rpx auto 0;
}
.comfirmBtn button {
    background-color: #93e9f3;
    border-radius: 8rpx;
    color: white;
    font-size: 30rpx;
    line-height: 85rpx;
}
.closeComment {
    margin: 20rpx auto 0;
    width: 64rpx;
}
.closeComment image {
    width: 100%;
}